# ============================================
# SERVICE - Separate pipelines per metric type
# ============================================
service:
  pipelines:
    # System metrics (uptime) - all routers
    metrics/system:
      receivers:
        - snmp/csr23_system
        - snmp/csr24_system
        - snmp/csr25_system
        - snmp/csr26_system
        - snmp/csr27_system
        - snmp/csr28_system
        - snmp/csr29_system
      processors:
        - batch
      exporters:
        - elasticsearch/system
        - debug
    
    # Memory metrics - csr28 only
    metrics/memory:
      receivers:
        - snmp/csr28_memory
      processors:
        - batch
      exporters:
        - elasticsearch/memory
        - debug
    
    # Interface metrics - all routers
    metrics/interface:
      receivers:
        - snmp/csr23_interface
        - snmp/csr24_interface
        - snmp/csr25_interface
        - snmp/csr26_interface
        - snmp/csr27_interface
        - snmp/csr28_interface
        - snmp/csr29_interface
      processors:
        - batch
      exporters:
        - elasticsearch/interface
        - debug
    
    # IP stats - csr28 only
    metrics/ipstats:
      receivers:
        - snmp/csr28_ipstats
      processors:
        - batch
      exporters:
        - elasticsearch/ipstats
        - debug
    
    # TCP metrics - csr28 only
    metrics/tcp:
      receivers:
        - snmp/csr28_tcp
      processors:
        - batch
      exporters:
        - elasticsearch/tcp
        - debug
    
    # UDP metrics - csr28 only
    metrics/udp:
      receivers:
        - snmp/csr28_udp
      processors:
        - batch
      exporters:
        - elasticsearch/udp
        - debug
    
    # ARP metrics - csr28 only
    metrics/arp:
      receivers:
        - snmp/csr28_arp
      processors:
        - batch
      exporters:
        - elasticsearch/arp
        - debug
    
    # LLDP metrics - all routers
    metrics/lldp:
      receivers:
        - snmp/csr23_lldp
        - snmp/csr24_lldp
        - snmp/csr25_lldp
        - snmp/csr26_lldp
        - snmp/csr27_lldp
        - snmp/csr28_lldp
        - snmp/csr29_lldp
      processors:
        - batch
      exporters:
        - elasticsearch/lldp
        - debug
  
  telemetry:
    logs:
      level: info
