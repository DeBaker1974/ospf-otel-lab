receivers:
  # CSR23
  snmp/csr23_system:
    endpoint: udp://172.20.20.23:161
    version: v2c
    community: public
    collection_interval: 30s
    resource_attributes:
      host.name:
        scalar_oid: "1.3.6.1.2.1.1.5.0"
    metrics:
      system.uptime:
        unit: s
        gauge:
          value_type: double
        scalar_oids:
          - oid: "1.3.6.1.2.1.1.3.0"
            resource_attributes:
              - host.name

  snmp/csr23_memory:
    endpoint: udp://172.20.20.23:161
