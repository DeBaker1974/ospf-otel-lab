            attributes:
              - name: if.index
              - name: if.name
              - name: if.descr
            resource_attributes:
              - host.name
      network.interface.out.bytes:
        unit: By
        sum:
          value_type: int
          monotonic: true
          aggregation: cumulative
        column_oids:
          - oid: "1.3.6.1.2.1.31.1.1.1.10"
            attributes:
              - name: if.index
              - name: if.name
              - name: if.descr
            resource_attributes:
              - host.name
      network.interface.in.errors:
        unit: "1"
        sum:
          value_type: int
          monotonic: true
          aggregation: cumulative
