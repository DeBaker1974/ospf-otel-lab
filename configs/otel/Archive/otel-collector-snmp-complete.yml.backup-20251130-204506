processors:
  resource/default:
    attributes:
      - key: data_stream.type
        value: metrics
        action: upsert
      - key: data_stream.dataset
        value: snmpreceiver
        action: upsert
      - key: data_stream.namespace
        value: prod
        action: upsert

  batch:
    timeout: 10s
    send_batch_size: 100

exporters:
  otlp:
    endpoint: edot-collector:4317
    tls:
      insecure: true

  debug:
    verbosity: normal
    sampling_initial: 2
    sampling_thereafter: 500

