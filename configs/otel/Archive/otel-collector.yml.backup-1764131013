receivers:
  # ============================================
  # CSR28 - Complete monitoring
  # ============================================
  snmp/csr28_system:
    endpoint: udp://172.20.20.28:161
    version: v2c
    community: public
    collection_interval: 10s
    timeout: 10s
    resource_attributes:
      host.name: { scalar_oid: "1.3.6.1.2.1.1.5.0" }
    metrics:
      system.uptime:
        unit: s
        gauge: { value_type: double }
        scalar_oids:
          - oid: "1.3.6.1.2.1.1.3.0"
            resource_attributes: ["host.name"]
  
  snmp/csr28_memory:
    endpoint: udp://172.20.20.28:161
    version: v2c
    community: public
    collection_interval: 30s
    timeout: 10s
    resource_attributes:
      host.name: { scalar_oid: "1.3.6.1.2.1.1.5.0" }
    attributes:
      memory.pool.name: { oid: "1.3.6.1.2.1.25.2.3.1.3" }
    metrics:
      system.memory.size:
        unit: units
        gauge: { value_type: int }
        column_oids:
