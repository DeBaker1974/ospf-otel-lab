# Service Pipelines - ALL ROUTERS
# ========================================
service:
  pipelines:
    # System metrics - ALL ROUTERS
    metrics/system:
      receivers:
        - snmp/csr23_system
        - snmp/csr24_system
        - snmp/csr25_system
        - snmp/csr26_system
        - snmp/csr27_system
        - snmp/csr28_system
        - snmp/csr29_system
      processors:
        - resource/system
        - batch
      exporters:
        - elasticsearch/system
        - debug
    
    # Memory metrics - ALL ROUTERS
    metrics/memory:
      receivers:
        - snmp/csr23_memory
        - snmp/csr24_memory
        - snmp/csr25_memory
        - snmp/csr26_memory
        - snmp/csr27_memory
        - snmp/csr28_memory
